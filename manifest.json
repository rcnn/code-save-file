{
  "manifest_version": 3,
  "name": "代码文件批量保存",
  "version": "1.0.0",
  "description": "从网页中提取代码文件并批量保存",
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "libs/jszip.min.js",
        "content.js"
      ],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png"
  }
}
